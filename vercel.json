```language:AlWeb/vercel.json
{
    "builds": [
        {
            "src": "AlWeb/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build" // Ensure this is correct
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "AlWeb/wsgi.py"
        },
        {
            "src": "/static/(.*)", // Added route for static files
            "dest": "/static/$1"
        }
    ]
}